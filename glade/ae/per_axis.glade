<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkBox" id="per_axis">
    <property name="margin-start">50</property>
    <property name="margin-end">50</property>
    <property name="margin_top">20</property>
    <property name="orientation">vertical</property>
    <property name="spacing">10</property>
    <child>
      <object class="GtkButton" id="btAxisX">
        <property name="focusable">1</property>
        <property name="receives_default">1</property>
        <property name="hexpand">1</property>
        <signal name="clicked" handler="on_btAxisX_clicked" swapped="no"/>
        <child>
          <object class="GtkBox" id="box8">
            <child>
              <object class="GtkLabel" id="label6">
                <property name="width_request">200</property>
                <property name="label" translatable="1">X Axis</property>
                <property name="xalign">0</property>
              </object>
            </child>
            <child>
              <object class="GtkSeparator" id="separator7"/>
            </child>
            <child>
              <object class="GtkLabel" id="lblAxisX">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">(...)</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="btAxisY">
        <property name="focusable">1</property>
        <property name="receives_default">1</property>
        <property name="hexpand">1</property>
        <signal name="clicked" handler="on_btAxisY_clicked" swapped="no"/>
        <child>
          <object class="GtkBox" id="box9">
            <child>
              <object class="GtkLabel" id="label8">
                <property name="width_request">200</property>
                <property name="label" translatable="1">Y Axis</property>
                <property name="xalign">0</property>
              </object>
            </child>
            <child>
              <object class="GtkSeparator" id="separator8"/>
            </child>
            <child>
              <object class="GtkLabel" id="lblAxisY">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">(...)</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
